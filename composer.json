{
    "name": "davegthemighty/designpatterns",
    "description": "Pootling around with design patterns using php/slim",
    "keywords": ["slim", "php", "design patterns"],
    "homepage": "http://www.github.com/davegthemighty/designpatterns",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave",
            "email": "dave@example.com",
            "homepage": "http://www.github.com/davegthemighty"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^3.3",
        "slim/twig-view": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "DesignPatterns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesignPatterns\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
